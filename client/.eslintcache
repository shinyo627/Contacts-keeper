"filePath":"110","messages":"111","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"58"},{"filePath":"112","messages":"113","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"58"},{"filePath":"114","messages":"115","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"58"},"/Users/yo/traversy/react_front_to_back/contacts_keeper/client/src/index.js",[],["116","117"],"/Users/yo/traversy/react_front_to_back/contacts_keeper/client/src/App.js",[],"/Users/yo/traversy/react_front_to_back/contacts_keeper/client/src/reportWebVitals.js",[],"/Users/yo/traversy/react_front_to_back/contacts_keeper/client/src/components/layouts/Navbar.js",[],"/Users/yo/traversy/react_front_to_back/contacts_keeper/client/src/components/pages/Home.js",[],"/Users/yo/traversy/react_front_to_back/contacts_keeper/client/src/components/pages/About.js",[],"/Users/yo/traversy/react_front_to_back/contacts_keeper/client/src/context/contact/ContactState.js",["118"],"import { useReducer } from 'react';\nimport axios from 'axios';\nimport ContactContext from './contactContext';\nimport contactReducer from './contactReducer';\n\nimport {\n  GET_CONTACTS,\n  ADD_CONTACT,\n  DELETE_CONTACT,\n  SET_CURRENT,\n  CLEAR_CURRENT,\n  UPDATE_CONTACT,\n  FILTER_CONTACT,\n  CLEAR_CONTACTS,\n  CLEAR_FILTER,\n  CONTACT_ERROR,\n} from '../types';\n\nconst ContactState = (props) => {\n  const initialState = {\n    contacts: null,\n    current: null,\n    filtered: null,\n    error: null,\n  };\n\n  const [state, dispatch] = useReducer(contactReducer, initialState);\n\n  // Get Contacts\n  const getContacts = async () => {\n    try {\n      const res = await axios.get('/api/contacts');\n\n      dispatch({\n        type: GET_CONTACTS,\n        payload: res.data,\n      });\n    } catch (err) {\n      dispatch({\n        type: CONTACT_ERROR,\n        payload: err.response.msg,\n      });\n    }\n  };\n\n  // Add Contact\n  const addContact = async (contact) => {\n    const config = {\n      headers: {\n        'Content-Type': 'application/json',\n      },\n    };\n\n    try {\n      const res = await axios.post('/api/contacts', contact, config);\n\n      dispatch({\n        type: ADD_CONTACT,\n        payload: res.data,\n      });\n    } catch (err) {\n      dispatch({\n        type: CONTACT_ERROR,\n        payload: err.response.msg,\n      });\n    }\n  };\n\n  // Clear contacts\n  const clearContacts = () => {\n    dispatch({\n      type: CLEAR_CONTACTS,\n    });\n  };\n\n  // Delete Contact\n  const deleteContact = async (id) => {\n    try {\n      const res = await axios.delete(`/api/contacts/${id}`);\n\n      dispatch({\n        type: DELETE_CONTACT,\n        payload: id,\n      });\n    } catch (err) {\n      dispatch({\n        type: CONTACT_ERROR,\n        payload: err.response.msg,\n      });\n    }\n  };\n\n  // Update Contact\n  const updateContact = async (contact) => {\n    const config = {\n      headers: {\n        'Content-Type': 'application/json',\n      },\n    };\n\n    try {\n      const res = await axios.put(\n        `/api/contacts/${contact._id}`,\n        contact,\n        config\n      );\n\n      dispatch({\n        type: UPDATE_CONTACT,\n        payload: res.data,\n      });\n    } catch (err) {\n      dispatch({\n        type: CONTACT_ERROR,\n        payload: err.response.msg,\n      });\n    }\n  };\n\n  // Set Current Contact\n  const setCurrent = (contact) => {\n    dispatch({\n      type: SET_CURRENT,\n      payload: contact,\n    });\n  };\n\n  // Clear Current Contact\n  const clearCurrent = () => {\n    dispatch({\n      type: CLEAR_CURRENT,\n    });\n  };\n\n  // Filter Contacts\n  const filterContacts = (keyword) => {\n    dispatch({\n      type: FILTER_CONTACT,\n      payload: keyword,\n    });\n  };\n\n  // Clear Filter\n  const clearFilter = () => {\n    dispatch({\n      type: CLEAR_FILTER,\n    });\n  };\n  return (\n    <ContactContext.Provider\n      value={{\n        contacts: state.contacts,\n        current: state.current,\n        filtered: state.filtered,\n        error: state.error,\n        getContacts,\n        addContact,\n        deleteContact,\n        setCurrent,\n        clearCurrent,\n        updateContact,\n        filterContacts,\n        clearFilter,\n        clearContacts,\n      }}\n    >\n      {props.children}\n    </ContactContext.Provider>\n  );\n};\n\nexport default ContactState;\n","/Users/yo/traversy/react_front_to_back/contacts_keeper/client/src/context/contact/contactReducer.js",["119"],"import {\n  GET_CONTACTS,\n  ADD_CONTACT,\n  DELETE_CONTACT,\n  CLEAR_CONTACTS,\n  SET_CURRENT,\n  CLEAR_CURRENT,\n  UPDATE_CONTACT,\n  FILTER_CONTACT,\n  CLEAR_FILTER,\n  CONTACT_ERROR,\n} from '../types';\n\nexport default (state, action) => {\n  switch (action.type) {\n    case GET_CONTACTS:\n      return {\n        ...state,\n        contacts: action.payload,\n        loading: false,\n      };\n    case ADD_CONTACT:\n      return {\n        //   Spreading current state\n        ...state,\n        // state is immutable so spread contacts again when adding incoming contact to spread state.contacts\n        contacts: [action.payload, ...state.contacts],\n        loading: false,\n      };\n    case UPDATE_CONTACT:\n      return {\n        ...state,\n        contacts: state.contacts.map((contact) =>\n          contact._id === action.payload._id ? action.payload : contact\n        ),\n        loading: false,\n      };\n    case DELETE_CONTACT:\n      return {\n        ...state,\n        contacts: state.contacts.filter(\n          (contact) => contact._id !== action.payload\n        ),\n        loading: false,\n      };\n    case CLEAR_CONTACTS:\n      return {\n        ...state,\n        contacts: null,\n        filtered: null,\n        error: null,\n        current: null,\n      };\n    case SET_CURRENT:\n      return {\n        ...state,\n        // Entire incoming contact object\n        current: action.payload,\n      };\n    case CLEAR_CURRENT:\n      return {\n        ...state,\n        current: null,\n      };\n    case FILTER_CONTACT:\n      return {\n        ...state,\n        filtered: state.contacts.filter((contact) => {\n          // gi means global and insensitive for matching. Below line returns text\n          const regex = new RegExp(`${action.payload}`, 'gi');\n          return contact.name.match(regex) || contact.email.match(regex);\n        }),\n      };\n    case CLEAR_FILTER:\n      return {\n        ...state,\n        filtered: null,\n      };\n    case CONTACT_ERROR:\n      return {\n        ...state,\n        error: action.payload,\n      };\n    default:\n      return state;\n  }\n};\n","/Users/yo/traversy/react_front_to_back/contacts_keeper/client/src/context/types.js",[],"/Users/yo/traversy/react_front_to_back/contacts_keeper/client/src/context/contact/contactContext.js",[],"/Users/yo/traversy/react_front_to_back/contacts_keeper/client/src/components/contacts/Contacts.js",[],"/Users/yo/traversy/react_front_to_back/contacts_keeper/client/src/components/contacts/ContactItem.js",["120"],"import { useContext } from 'react';\nimport PropTypes from 'prop-types';\nimport ContactContext from '../../context/contact/contactContext';\n\nconst ContactItem = ({ contact }) => {\n  const contactContext = useContext(ContactContext);\n  const { current, deleteContact, setCurrent, clearCurrent } = contactContext;\n\n  const { _id, name, email, phone, type } = contact;\n\n  const onDelete = () => {\n    // Using id originates from mongoDB field for each documents of contact\n    deleteContact(_id);\n    clearCurrent();\n  };\n  return (\n    <div className='card bg-light '>\n      <h3 className='text-primary text-left'>\n        {name}{' '}\n        <span\n          style={{ float: 'right' }}\n          className={\n            'badge ' +\n            (type === 'professional' ? 'badge-success' : 'badge-primary')\n          }\n        >\n          {type.charAt(0).toUpperCase() + type.slice(1)}\n        </span>\n      </h3>\n\n      <ul className='list'>\n        {email && (\n          <li>\n            <i className='fas fa-envelope-open'>{email}</i>\n          </li>\n        )}\n        {phone && (\n          <li>\n            <i className='fas fa-phone'>{phone}</i>\n          </li>\n        )}\n      </ul>\n\n      <p>\n        <button\n          className='btn btn-dark btn-sm'\n          onClick={() => {\n            setCurrent(contact);\n          }}\n        >\n          Edit\n        </button>\n        <button className='btn btn-danger btn-sm' onClick={onDelete}>\n          Delete\n        </button>\n      </p>\n    </div>\n  );\n};\n\nContactItem.propTypes = {\n  contact: PropTypes.object.isRequired,\n};\n\nexport default ContactItem;\n","/Users/yo/traversy/react_front_to_back/contacts_keeper/client/src/components/contacts/ContactForm.js",[],"/Users/yo/traversy/react_front_to_back/contacts_keeper/client/src/components/contacts/contactFilter.js",["121","122","123"],"/Users/yo/traversy/react_front_to_back/contacts_keeper/client/src/components/contacts/ContactFilter.js",[],"/Users/yo/traversy/react_front_to_back/contacts_keeper/client/src/context/auth/AuthState.js",[],"/Users/yo/traversy/react_front_to_back/contacts_keeper/client/src/context/auth/authContext.js",[],"/Users/yo/traversy/react_front_to_back/contacts_keeper/client/src/context/auth/authReducer.js",["124"],"import {\n  REGISTER_SUCCESS,\n  REGISTER_FAIL,\n  USER_LOADED,\n  AUTH_ERROR,\n  LOGIN_SUCCESS,\n  LOGIN_FAIL,\n  LOGOUT,\n  CLEAR_ERRORS,\n} from '../types';\n\nexport default (state, action) => {\n  switch (action.type) {\n    case USER_LOADED:\n      return {\n        ...state,\n        isAuthenticated: true,\n        loading: false,\n        user: action.payload,\n      };\n    case REGISTER_SUCCESS:\n    case LOGIN_SUCCESS:\n      // Putting incoming/fetched token into localStorage firstly so that loadUser() will be triggered successfully\n      localStorage.setItem('token', action.payload.token);\n      return {\n        // Spreading current state\n        ...state,\n        // Putting fetched token to token state\n        ...action.payload,\n        isAuthenticated: true,\n        loading: false,\n      };\n    case REGISTER_FAIL:\n    case AUTH_ERROR:\n    case LOGIN_FAIL:\n    case LOGOUT:\n      // Removing token in case if Register process fail\n      localStorage.removeItem('token');\n      return {\n        ...state,\n        token: null,\n        isAuthenticated: false,\n        loading: false,\n        user: null,\n        error: action.payload,\n      };\n    case CLEAR_ERRORS:\n      return {\n        ...state,\n        error: null,\n      };\n    default:\n      return state;\n  }\n};\n","/Users/yo/traversy/react_front_to_back/contacts_keeper/client/src/components/auth/Register.js",[],"/Users/yo/traversy/react_front_to_back/contacts_keeper/client/src/components/auth/Login.js",[],"/Users/yo/traversy/react_front_to_back/contacts_keeper/client/src/context/alert/AlertState.js",[],"/Users/yo/traversy/react_front_to_back/contacts_keeper/client/src/context/alert/alertReducer.js",["125"],"import { SET_ALERT, REMOVE_ALERT } from '../types';\n\nexport default (state, action) => {\n  switch (action.type) {\n    case SET_ALERT:\n      return [...state, action.payload];\n    case REMOVE_ALERT:\n      return state.filter((alert) => alert.id !== action.payload);\n    default:\n      return state;\n  }\n};\n","/Users/yo/traversy/react_front_to_back/contacts_keeper/client/src/context/alert/alertContext.js",[],"/Users/yo/traversy/react_front_to_back/contacts_keeper/client/src/components/layouts/Alerts.js",[],"/Users/yo/traversy/react_front_to_back/contacts_keeper/client/src/utils/setAuthToken.js",[],"/Users/yo/traversy/react_front_to_back/contacts_keeper/client/src/components/routing/PrivateRoute.js",[],"/Users/yo/traversy/react_front_to_back/contacts_keeper/cl