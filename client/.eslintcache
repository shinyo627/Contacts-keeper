/components/pages/About.js",[],"/Users/yo/traversy/react_front_to_back/contacts_keeper/client/src/context/contact/ContactState.js",[],"/Users/yo/traversy/react_front_to_back/contacts_keeper/client/src/context/contact/contactReducer.js",["107"],"import {\n  ADD_CONTACT,\n  DELETE_CONTACT,\n  SET_CURRENT,\n  CLEAR_CURRENT,\n  UPDATE_CONTACT,\n  FILTER_CONTACT,\n  CLEAR_FILTER,\n} from '../types';\n\nexport default (state, action) => {\n  switch (action.type) {\n    case ADD_CONTACT:\n      return {\n        //   Spreading current state\n        ...state,\n        // state is immutable so spread contacts again when adding incoming contact to spread state.contacts\n        contacts: [...state.contacts, action.payload],\n      };\n    case UPDATE_CONTACT:\n      return {\n        ...state,\n        contacts: state.contacts.map((contact) =>\n          contact.id === action.payload.id ? action.payload : contact\n        ),\n      };\n    case DELETE_CONTACT:\n      return {\n        ...state,\n        contacts: state.contacts.filter(\n          (contact) => contact.id !== action.payload\n        ),\n      };\n    case SET_CURRENT:\n      return {\n        ...state,\n        // Entire incoming contact object\n        current: action.payload,\n      };\n    case CLEAR_CURRENT:\n      return {\n        ...state,\n        current: null,\n      };\n    case FILTER_CONTACT:\n      return {\n        ...state,\n        filtered: state.contacts.filter((contact) => {\n          // gi means global and insensitive for matching. Below line returns text\n          const regex = new RegExp(`${action.payload}`, 'gi');\n          return contact.name.match(regex) || contact.email.match(regex);\n        }),\n      };\n    case CLEAR_FILTER:\n      return {\n        ...state,\n        filtered: null,\n      };\n    default:\n      return state;\n  }\n};\n","/Users/yo/traversy/react_front_to_back/contacts_keeper/client/src/context/types.js",[],"/Users/yo/traversy/react_front_to_back/contacts_keeper/client/src/context/contact/contactContext.js",[],"/Users/yo/traversy/react_front_to_back/contacts_keeper/client/src/components/contacts/Contacts.js",[],"/Users/yo/traversy/react_front_to_back/contacts_keeper/client/src/components/contacts/ContactItem.js",["108"],"import { useContext } from 'react';\nimport PropTypes from 'prop-types';\nimport ContactContext from '../../context/contact/contactContext';\n\nconst ContactItem = ({ contact }) => {\n  const contactContext = useContext(ContactContext);\n  const { current, deleteContact, setCurrent, clearCurrent } = contactContext;\n\n  const { id, name, email, phone, type } = contact;\n\n  const onDelete = () => {\n    // Using id originates from mongoDB field for each documents of contact\n    deleteContact(id);\n    clearCurrent();\n  };\n  return (\n    <div className='card bg-light '>\n      <h3 className='text-primary text-left'>\n        {name}{' '}\n        <span\n          style={{ float: 'right' }}\n          className={\n            'badge ' +\n            (type === 'professional' ? 'badge-success' : 'badge-primary')\n          }\n        >\n          {type.charAt(0).toUpperCase() + type.slice(1)}\n        </span>\n      </h3>\n\n      <ul className='list'>\n        {email && (\n          <li>\n            <i className='fas fa-envelope-open'>{email}</i>\n          </li>\n        )}\n        {phone && (\n          <li>\n            <i className='fas fa-phone'>{phone}</i>\n          </li>\n        )}\n      </ul>\n\n      <p>\n        <button\n          className='btn btn-dark btn-sm'\n          onClick={() => {\n            setCurrent(contact);\n          }}\n        >\n          Edit\n        </button>\n        <button className='btn btn-danger btn-sm' onClick={onDelete}>\n          Delete\n        </button>\n      </p>\n    </div>\n  );\n};\n\nContactItem.propTypes = {\n  contact: PropTypes.object.isRequired,\n};\n\nexport default ContactItem;\n","/Users/yo/traversy/react_front_to_back/contacts_keeper/clie