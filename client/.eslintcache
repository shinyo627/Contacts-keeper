e,\n        current: null,\n      };\n    case FILTER_CONTACT:\n      return {\n        ...state,\n        filtered: state.contacts.filter((contact) => {\n          // gi means global and insensitive for matching. Below line returns text\n          const regex = new RegExp(`${action.payload}`, 'gi');\n          return contact.name.match(regex) || contact.email.match(regex);\n        }),\n      };\n    case CLEAR_FILTER:\n      return {\n        ...state,\n        filtered: null,\n      };\n    default:\n      return state;\n  }\n};\n","/Users/yo/traversy/react_front_to_back/contacts_keeper/client/src/context/types.js",[],"/Users/yo/traversy/react_front_to_back/contacts_keeper/client/src/context/contact/contactContext.js",[],"/Users/yo/traversy/react_front_to_back/contacts_keeper/client/src/components/contacts/Contacts.js",[],"/Users/yo/traversy/react_front_to_back/contacts_keeper/client/src/components/contacts/ContactItem.js",["90"],"import { useContext } from 'react';\nimport PropTypes from 'prop-types';\nimport ContactContext from '../../context/contact/contactContext';\n\nconst ContactItem = ({ contact }) => {\n  const contactContext = useContext(ContactContext);\n  const { current, deleteContact, setCurrent, clearCurrent } = contactContext;\n\n  const { id, name, email, phone, type } = contact;\n\n  const onDelete = () => {\n    // Using id originates from mongoDB field for each documents of contact\n    deleteContact(id);\n    clearCurrent();\n  };\n  return (\n    <div className='card bg-light '>\n      <h3 className='text-primary text-left'>\n        {name}{' '}\n        <span\n          style={{ float: 'right' }}\n          className={\n            'badge ' +\n            (type === 'professional' ? 'badge-success' : 'badge-primary')\n          }\n        >\n          {type.charAt(0).toUpperCase() + type.slice(1)}\n        </span>\n      </h3>\n\n      <ul className='list'>\n        {email && (\n          <li>\n            <i className='fas fa-envelope-open'>